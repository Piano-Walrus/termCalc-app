<?xml version="1.0" encoding="utf-8"?>
<ripple
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/rippleWhite">

    <item android:id="@android:id/mask">
        <shape
            android:shape="oval">
        <solid
            android:color="@color/darkColorPrimaryDark"/>

        <size
            android:width="60dp"
            android:height="60dp"/>
        </shape>
    </item>

    <item android:id="@android:id/background">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/darkColorPrimaryDark"/>

            <size
                android:width="60dp"
                android:height="60dp"/>
        </shape>
    </item>
</ripple>
